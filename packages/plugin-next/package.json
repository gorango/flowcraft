{
	"name": "@flowcraft/next-plugin",
	"type": "commonjs",
	"private": true,
	"version": "0.0.0",
	"description": "Next.js plugin for Flowcraft compiler integration",
	"license": "MIT",
	"homepage": "https://flowcraft.js.org",
	"repository": {
		"type": "git",
		"url": "https://github.com/gorango/flowcraft.git"
	},
	"bugs": "https://github.com/gorango/flowcraft/issues",
	"keywords": [
		"flowcraft",
		"nextjs",
		"next",
		"plugin",
		"build",
		"workflow"
	],
	"exports": {
		".": {
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"typecheck": "tsc --noEmit",
		"test": "echo \"Error: no test specified\" && exit 0"
	},
	"dependencies": {
		"@flowcraft/compiler": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "^24.9.2",
		"tsup": "^8.5.0"
	},
	"peerDependencies": {
		"next": "^13.0.0 || ^14.0.0 || ^15.0.0"
	}
}
